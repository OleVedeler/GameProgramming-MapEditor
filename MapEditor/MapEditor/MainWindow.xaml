<Window x:Class="MapEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="500" Width="750">
    <Grid>
        <Menu IsMainMenu="True">
            <MenuItem Header="_File">
                <MenuItem x:Name="New" Header="_New" Command="New" Click="MenuItem_new" />
                <MenuItem x:Name="Save" Header="_Save" Command="Save" Click="MenuItem_save" />
                <MenuItem x:Name="Load" Header="_Load" Command="Open" Click="MenuItem_load" />
                <Separator />
                <MenuItem x:Name="Import" Header="_Import Asset" Click="MenuItem_import" />
                <Separator />
                <MenuItem x:Name="Exit" Header="_Exit" Click="MenuItem_exit" />
            </MenuItem>
        </Menu>
        <Grid Name="EditorGrid" HorizontalAlignment="Left" Height="440" Margin="175,23,0,0" VerticalAlignment="Top" Width="557"/>
        <TreeView Name="ComponentsTreeView" HorizontalAlignment="Left" Height="336" Margin="10,23,0,0" VerticalAlignment="Top" Width="147"/>
    </Grid>
</Window>
